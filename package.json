{
  "name": "@starzkg/vuepress-monorepo",
  "private": true,
  "scripts": {
    "build": "pnpm build:all && pnpm build:copy",
    "build:all": "pnpm --filter=!@starzkg/vuepress-monorepo run -r --stream build",
    "build:copy": "pnpm run -r --stream copy",
    "build:tsc": "tsc -b tsconfig.build.json",
    "build:tsup": "pnpm --filter=@vuepress/client --filter=@vuepress/shared --parallel build",
    "clean": "pnpm run -r --stream clean",
    "dev:copy": "pnpm --filter=* --parallel copy --watch",
    "dev:tsc": "pnpm build:tsc --watch",
    "dev:tsup": "pnpm build:tsup --watch",
    "docs:build": "pnpm --filter=docs docs:build",
    "docs:clean": "pnpm --filter=docs docs:clean",
    "docs:dev": "pnpm --filter=docs docs:dev",
    "docs:release": "pnpm build && pnpm docs:build",
    "docs:serve": "pnpm --filter=docs docs:serve",
    "example:build": "pnpm --filter=example example:build",
    "example:clean": "pnpm --filter=example example:clean",
    "example:dev": "pnpm --filter=example example:dev",
    "example:release": "pnpm build && pnpm example:build",
    "example:serve": "pnpm --filter=docs docs:serve",
    "lint": "eslint --ext .js,.ts,.vue .",
    "prepare": "husky install",
    "rebuild": "pnpm clean && pnpm copy && pnpm build",
    "release": "pnpm release:check && pnpm release:version && pnpm release:publish",
    "release:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release:check": "pnpm lint && pnpm clean && pnpm build && pnpm test",
    "release:publish": "pnpm -r publish --tag next",
    "release:version": "bumpp package.json packages/*/package.json packages/@vuepress/*/package.json --execute=\"pnpm release:changelog\" --commit \"build: publish v%s\" --all --tag --push",
    "test": "jest --runInBand",
    "test:coverage": "pnpm copy && pnpm build && pnpm test --coverage"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix",
    "*.{json,yml,css,scss}": "prettier --write",
    "package.json": "sort-package-json"
  },
  "prettier": "prettier-config-vuepress",
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@babel/plugin-transform-modules-commonjs": "7.17.9",
    "@babel/preset-env": "7.17.10",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@rollup/plugin-typescript": "8.3.0",
    "@types/jest": "^27.4.1",
    "@types/node": "17.0.32",
    "@types/webpack-env": "^1.16.4",
    "bumpp": "^7.1.1",
    "conventional-changelog-cli": "^2.2.2",
    "cpx2": "^4.2.0",
    "eslint": "^8.14.0",
    "eslint-config-vuepress": "^3.7.1",
    "eslint-config-vuepress-typescript": "^2.7.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^12.4.0",
    "prettier": "^2.6.2",
    "prettier-config-vuepress": "^1.4.0",
    "rimraf": "^3.0.2",
    "rollup": "2.72.1",
    "sort-package-json": "^1.55.0",
    "ts-jest": "^27.1.4",
    "ts-node": "10.7.0",
    "tsconfig-vuepress": "^0.1.0",
    "tslib": "2.4.0",
    "tsup": "^5.12.5",
    "typescript": "^4.6.3",
    "vite": "~2.9.5"
  },
  "engines": {
    "node": ">=14",
    "pnpm": ">=6"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "eslint-plugin-import",
        "eslint-plugin-n",
        "eslint-plugin-promise",
        "react",
        "react-dom",
        "webpack"
      ]
    }
  }
}
