{
  "name": "@starzkg/vuepress-icons",
  "version": "1.0.0-beta.28",
  "description": "Icon library for Vuepress",
  "keywords": [
    "icon",
    "svg",
    "vuepress"
  ],
  "homepage": "https://vuepress-star.shentuzhigang.cn",
  "bugs": {
    "url": "https://github.com/vuepress-star/vuepress-theme-star/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress-star/vuepress-theme-star.git",
    "directory": "packages/@starzkg/vuepress-icons"
  },
  "license": "MIT",
  "author": {
    "name": "Starzkg",
    "email": "shentuzhigang@shentuzhigang.cn",
    "url": "https://shentuzhigang.cn"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./global": "./lib/global.js",
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "main": "lib/index.js",
  "unpkg": "lib/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "files": [
    "es",
    "lib",
    "types"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "rimraf lib *.tsbuildinfo",
    "copy": "cpx \"src/**/*.vue\" lib",
    "gen": "node ./scripts/generate.js && yarn prettier",
    "prettier": "prettier ./src -w"
  },
  "dependencies": {
    "chalk": "^5.0.0",
    "ora": "^5.4.1",
    "svgo": "^2.3.1",
    "vue": "^3.2.40"
  },
  "publishConfig": {
    "access": "public"
  }
