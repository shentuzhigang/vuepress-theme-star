    "@vuepress/utils": "2.0.0-beta.45",
    "chalk": "^4.1.2",
    "copy-to-clipboard": "^3.3.1",
    "dayjs": "^1.11.2",
    "execa": "^5.1.1",
    "ora": "^5.4.1",
    "vue": "^3.2.35",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@types/connect": "^3.4.35",
    "@vuepress/bundler-vite": "2.0.0-beta.45",
