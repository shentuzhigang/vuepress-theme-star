  "main": "lib/node/index.js",
  "module": "es/client/index.js",
  "types": "lib/node/index.d.ts",
  "files": [
    "es",
    "lib",
    "styles"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "rimraf lib es *.tsbuildinfo",
    "copy": "pnpm copy1 && pnpm copy2",
